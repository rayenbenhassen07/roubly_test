datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

model Post {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  date        DateTime
  time        DateTime
  firstName   String
  lastName    String
  email       String
  phoneNumber String?
  country     String?
}